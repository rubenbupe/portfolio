---
import type { HTMLAttributes } from "astro/types";
import { cn } from "../lib/utils";

type Props = HTMLAttributes<"section">;

const { class: className, ...rest } = Astro.props;
---

<section
    class={cn("w-full max-w-6xl px-6 md:px-12 self-center", className)}
    {...rest}
>
    <slot />
</section>
